<div class="contiener">
    <div class="form-contiener">
        
        <form class="signIn" id="signIn">
            <div class="add-post-title">
                <h2>Sign Up</h2>
            </div>
            <div class="error"></div>
            <label for="username">Username:</label>
            <input type="text" name="username" id="username">
            <label for="email">Email:</label>
            <input type="text" name="email" id="email">
            <label for="password">Password:</label>
            <input type="password" name="password" id="password">
            <div class="add-post-button"> 
                <button>Sign Up</button>
            </div>
        </form>
    </div>
</div>

<script>

        const formSinup = document.getElementById('signIn');
        
        
        formSinup.addEventListener('submit',  async function(e) {
            e.preventDefault();
        
        
            const username  =  formSinup.username.value;
            const email     =  formSinup.email.value;
            const password  =  formSinup.password.value;
        
            console.log(username, email, password)
        
            try {
            const res = await fetch('/singup', {
                method: 'POST',
                body: JSON.stringify( {username, email, password} ),
                headers: {'Content-Type': 'application/json'}
            });
            
            const data = await res.json();
            console.log(data)
            
            } catch (error) {
                console.log(error)
            }
        
            location.assign('/')
        })
    
    </script>